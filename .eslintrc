{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-multi-assign": "off",
    "max-len": ["error", 150],
    "new-cap": [2, {
      "capIsNewExceptions": ["Velocity"]
    }],
    "import/no-extraneous-dependencies": ["error", {
      devDependencies: [
        'karma.conf.js',
        'rollup.config.js',
        'examples/gulpfile.js'
      ]
    }]
  }
}
